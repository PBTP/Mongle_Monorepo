{
  "name": "@mgmg/workspace",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "build:wb": "turbo build --filter=mg-wb",
    "build:bs": "turbo build --filter=mg-bs",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "mg:wb": "turbo dev --filter=mg-wb",
    "mg:bs": "turbo dev --filter=mg-bs",
    "start:wb": "turbo start --filter=mg-wb",
    "start:bs": "turbo start --filter=mg-bs"
  },
  "devDependencies": {
    "@types/node": "^22.10.6",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "turbo": "^2.1.1"
  },
  "packageManager": "pnpm@8.15.6"
}
